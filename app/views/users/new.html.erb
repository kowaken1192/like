<%= form_with model: @user do |f| %>
  <table>
    <tr>
      <th><%= f.label :title ,"タイトル" %></th>
      <td><%= f.text_field :title %></td>
    </tr>

    <tr>
     <th><%= f.label :start_date ,"開始日" %></th>
     <td><%= f.date_select :start_date, class: "datepicker"%></td>
    </tr>

    <tr>
     <th><%= f.label :end_date ,"終了日" %></th>
     <td><%= f.date_select :end_date,class: "datepicker" %></td>
    </tr>

    <tr>
     <th><%= f.label :schedule,"スケジュールメモ" %></th>
     <td><%= f.text_area :schedule %></td>
      
     </tr>

    

   
  </table>

  <div>
    <ul>
      <li><%= f.submit "登録完了する"%></li>
      <li><%= link_to "ユーザー情報一覧に戻る", :users %></li>
    </ul>
  </div>
<% end %>